{
  "name": "abappretty",
  "description": "ABAP pretty printer - Command line utility to format ABAP code on a server",
  "version": "0.2.2",
  "author": "Marcello Urbani @marcellourbani",
  "bin": {
    "abapPretty": "./bin/run"
  },
  "bugs": "https://github.com/marcellourbani/abapPretty/issues",
  "dependencies": {
    "@abaplint/core": "^2.32.10",
    "@oclif/command": "^1.5.20",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "abap-adt-api": "^1.3.0",
    "chalk": "^4.0.0",
    "cli-ux": "^5.4.5",
    "conf": "^6.2.4",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.21",
    "@types/request-debug": "^0.2.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/marcellourbani/abapPretty",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "abapPretty",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "marcellourbani/abapPretty",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
